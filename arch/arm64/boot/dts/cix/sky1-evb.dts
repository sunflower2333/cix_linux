	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_substrate_i2s2>;
	dmas = <&audss_dmac 6 0xff>;
	dma-names = "tx";
	cdns,pin-rx-mask = /bits/ 8 <0x00>;
	cdns,pin-tx-mask = /bits/ 8 <0x3c>; /* 0x3c, pin2-pin5 works as transmitter */
	status = "okay";
};

