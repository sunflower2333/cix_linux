	/* i2s_mc2b for dp-in */
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_substrate_i2s3>;
	dmas = <&audss_dmac 9 0xff>;
	dma-names = "rx";
	cdns,pin-rx-mask = /bits/ 8 <0x0f>; /* 0x0f, pin0-pin3 works as receiver */
	cdns,pin-tx-mask = /bits/ 8 <0x00>;
	status = "disabled";
