/*uart0 for BT*/
&uart0{
        pinctrl-names = "default";
        timeout-value = <10000>;
        pinctrl-0 = <&pinctrl_fch_uart0>;
        dmas = <&fch_dmac 2 0>, <&fch_dmac 3 1>;
        dma-names = "tx","rx";
		status = "okay";
};

