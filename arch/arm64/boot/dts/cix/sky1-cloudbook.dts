&usbc_phy0 {
	orientation-switch;
	svid = <0xff01>;
	default_conf = /bits/ 8 <0x03>;
	status = "okay";
	mode-switch;
	port {
		#address-cells = <1>;
		#size-cells = <0>;
		usbdpphy0_lane_switch: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&usbc0_phy_sw>;
		};
	};
};

