/********** HID_EC **********/
&i2c5 {
        status = "okay";
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_fch_i2c5>;
        clock-frequency = <100000>;
        keyboard_0: keyboard@3a {
                compatible = "hid-over-i2c";
                reg = <0x3a>;
                interrupt-parent = <&s5_gpio0>;
                interrupts = <0 IRQ_TYPE_EDGE_RISING>;
                hid-descr-addr = <0x0001>;
                status = "okay";
        };
};

